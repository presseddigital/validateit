{
    "name": "presseddigital/validateit",
    "description": "Supercharged text field validation.",
    "type": "craft-plugin",
    "version": "3.0.0",
    "keywords": [
        "craftcms",
        "validation",
        "validate"
    ],
    "support": {
        "docs": "https://github.com/presseddigital/validateit/blob/v3/README.md",
        "issues": "https://github.com/presseddigital/validateit/issues?state=open"
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "Pressed Digital",
            "homepage": "https://pressed.digital"
        }
    ],
    "require": {
        "craftcms/cms": "^3.1.8",
        "php": "^7.2.5|^8.0"
    },
    "autoload": {
        "psr-4": {
          "presseddigital\\validateit\\": "src/",
          "fruitstudios\\validateit\\": "src_legacy/"
        }
    },
    "extra": {
        "name": "Validateit",
        "handle": "validateit",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/presseddigital/validateit/v3/CHANGELOG.md",
        "class": "presseddigital\\validateit\\Validateit"
    }
}
